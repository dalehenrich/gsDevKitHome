#=========================================================================

usage() {
  cat <<HELP
USAGE: $(basename $0) [-h] <stone-name>
Update the todeClient images for my gemtalk branch.

OPTIONS
  -h display help

EXAMPLES
  $(basename $0) -h

HELP
}

set -e # exit on error
if [ "${GS_HOME}x" = "x" ] ; then
  echo "the GS_HOME environment variable needs to be defined"; exit 1
fi
source ${GS_HOME}/bin/shFunctions

if [ $# -ne 0 ]; then
  usage; exit 1
fi

$GS_HOME/bin/updateTodeImage

$GS_HOME/bin/updateTodeImage -p _0

$GS_HOME/bin/updateTodeImage -p _1

$GS_HOME/bin/updateTodeImage -p _2

$GS_HOME/bin/updateTodeImage -p _small

$GS_HOME/bin/updateTodeImage -p _small_0

$GS_HOME/bin/updateTodeImage -p _small_1

$GS_HOME/bin/updateTodeImage -p _small_2

$GS_HOME/bin/updateTodeImage -p _medium

$GS_HOME/bin/updateTodeImage -p _medium_0

$GS_HOME/bin/updateTodeImage -p _medium_1

$GS_HOME/bin/updateTodeImage -p _medium_2

$GS_HOME/bin/updateTodeImage -p _server

$GS_HOME/bin/updateTodeImage -p _client
